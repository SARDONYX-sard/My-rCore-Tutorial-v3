{
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.padding.bottom": 250,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    },
    "editor.scrollBeyondLastLine": false,
    "editor.wordSeparators": "./\\<)\"'>:,.;<>~!@# ○$%^&*|+=[]{}`~?．。，、（）「」［］｛｝《》てにをはがのともへでや"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "__alltraps",
    "ASID",
    "auipc",
    "binutils",
    "bitflags",
    "clippy",
    "cmdline",
    "condvar",
    "csrw",
    "dccache",
    "dealloc",
    "ebss",
    "ecall",
    "edata",
    "ekernel",
    "erodata",
    "errno",
    "etext",
    "filea",
    "fileb",
    "Fredrik",
    "GETCHAR",
    "getpid",
    "gettid",
    "icache",
    "idxs",
    "incbin",
    "initproc",
    "inlateout",
    "inodes",
    "kstack",
    "lookaside",
    "MMIO",
    "mtvec",
    "mutatorily",
    "noreturn",
    "objcopy",
    "PLIC",
    "ppid",
    "Proberen",
    "putchar",
    "RAII",
    "Rcore",
    "RDONLY",
    "RDWR",
    "refmut",
    "repr",
    "riscv",
    "rodata",
    "rustfmt",
    "rustup",
    "satp",
    "sbss",
    "scause",
    "sdata",
    "sepc",
    "SFENCE",
    "SIGABRT",
    "sigaction",
    "SIGALRM",
    "SIGBUS",
    "SIGCHLD",
    "SIGCONT",
    "SIGDEF",
    "SIGFPE",
    "SIGHUP",
    "SIGILL",
    "SIGINT",
    "SIGIO",
    "SIGKILL",
    "signum",
    "SIGPIPE",
    "sigprocmask",
    "SIGPROF",
    "SIGPWR",
    "SIGQUIT",
    "sigreturn",
    "SIGSEGV",
    "SIGSTKFLT",
    "SIGSTOP",
    "SIGSYS",
    "SIGTERM",
    "SIGTRAP",
    "SIGTSTP",
    "SIGTTIN",
    "SIGTTOU",
    "SIGURG",
    "SIGUSR1",
    "SIGUSR2",
    "SIGVTALRM",
    "SIGWINCH",
    "SIGXCPU",
    "SIGXFSZ",
    "sret",
    "srodata",
    "sscratch",
    "sstatus",
    "stackful",
    "stext",
    "stie",
    "stimer",
    "strampoline",
    "stval",
    "stvec",
    "SUCC",
    "syscall",
    "syscalls",
    "trapframe",
    "TRUNC",
    "uart",
    "unborrowed",
    "uniprocessor",
    "unmap",
    "unmapping",
    "userspace",
    "ustack",
    "vaddr",
    "Verhogen",
    "Virt",
    "virtio",
    "vtable",
    "waitpid",
    "waittid",
    "wakeup",
    "WRONLY",
    "xstate"
  ],
  "editor.formatOnSave": true,
  "files.associations": {
    "*.asm": "riscv",
    "*.S": "riscv"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "files.exclude": {
    "**/.dccache": true,
    "**/.DS_Store": true,
    "**/.gdb_history": true,
    "**/.git": true,
    "**/*.bk": true,
    "**/CVS": true,
    "**/last-qemu": true,
    "**/target": true
  },
  "git.autoRepositoryDetection": false,
  "git.ignoreLimitWarning": true,
  "markdownlint.config": {
    "default": true,
    "MD024": false,
    "MD025": false,
    "MD033": false,
    "no-hard-tabs": false
  },
  "rust-analyzer.cargo.features": [
    "board_qemu"
  ],
  "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.imports.prefer.no.std": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "rust-analyzer.rustfmt.rangeFormatting.enable": true
}
